@model List<string>
<div class="timeContainer">
    <div class="backDepth" onclick="closeModal()"></div>
    <div class="cont">
        <div class="contText">Выберите свободное время</div>
        <div class="times">
            @if (Model.Count() > 0)
            {
                @foreach (var time in Model)
                {
                    <div class="timeBoxx" onclick="changeTime(this)">@time</div>
                }
            }
            else
            {
                <div class="timeBoxx" onclick="closeModal()">Времени для записи нет!</div>
            }
        </div>
    </div>
</div>